set -x

viewpad10_info()
{
	# common for all Eee PC models
	PREDEV=snd-hda-codec-realtek
	SNDDEV="snd-hda-intel model=3stack-dig"
        CAMDEV=uvcvideo
	FB0DEV=i915
	WIFDEV=ath9k

	board=`cat /sys/class/dmi/id/product_name`

	case "$board" in
		PX1)
			WIFDEV=rt2860sta
			;;
		*)
			;;
	esac
        return 0 
}

detect_hardware()
{

	case "`cat /sys/class/dmi/id/uevent`" in
		*VPAD10*)
			viewpad10_info
			;;
		*)
			echo "error WARNING: Not a ViewSonic ViewPad 10"
			;;
	esac


}

